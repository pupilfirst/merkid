<h2>Apply to Fellowship</h2>

<%= form_tag(students_path) do %>
  <div>
    <p>email:</p>
    <%= email_field "student", "email", readonly: true, value: @email %>
  </div>
  <div>
    <p>First name:</p>
    <%= text_field "student", "first_name" %>
  </div>
  <p>
    <%= submit_tag "Submit" %>
  </p>
<% end %>


